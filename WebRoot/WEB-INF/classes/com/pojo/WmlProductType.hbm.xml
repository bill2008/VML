<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.pojo.WmlProductType" table="wml_product_type" dynamic-insert="true"  catalog="wml_test">
        <id name="id" type="java.lang.Integer">
            <column name="id" length="14" not-null="true"/>
            	<generator class="identity"></generator>
        </id>
         <property name="name" type="java.lang.String">
            <column name="name" length="255" />
        </property>
 	<property name="isDel" type="java.lang.Integer">
            <column name="isDel" length="11" />
        </property>
    </class>
</hibernate-mapping>
