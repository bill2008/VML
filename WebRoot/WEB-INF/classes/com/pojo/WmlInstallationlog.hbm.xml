<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.pojo.WmlInstallationlog" table="wml_installationlog" dynamic-insert="true"  catalog="wml_test">
        <id name="id" type="java.lang.Integer">
            <column name="id" length="14" not-null="true"/>
            	<generator class="identity"></generator>
        </id>
        <property name="createDate" type="java.lang.String">
            <column name="CreateDate" length="20" />
        </property>
        <property name="count" type="java.lang.Integer">
            <column name="count" length="14" />	
        </property>
        <property name="mac" type="java.lang.String">
            <column name="mac" length="255" />
        </property>
        <property name="region" type="java.lang.String">
            <column name="region" length="255" />
        </property>
         <property name="type" type="java.lang.String">
            <column name="type" length="255" />
        </property>
         <property name="isDel" type="java.lang.Integer">
            <column name="isDel" length="11" />	
        </property>
    </class>
</hibernate-mapping>
